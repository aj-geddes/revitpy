{
  "RevitPy": {
    "PythonPath": "",
    "PythonVersion": "3.11",
    "PythonLibPath": "",
    "PythonPaths": [],
    "MaxInterpreters": 5,
    "PythonTimeout": 30000,
    "EnableHotReload": true,
    "DebugServerPort": 8080,
    "EnableDebugServer": true,
    "TempDirectory": "",
    "LogDirectory": "",
    "MaxLogFileSizeMB": 50,
    "MaxLogFiles": 10,
    "EnableMemoryProfiling": false,
    "MemoryCheckInterval": 5000,
    "MaxMemoryUsageMB": 1024,
    "EnableSandbox": true,
    "AllowedFilePaths": [],
    "BlockedModules": [
      "os",
      "subprocess",
      "shutil",
      "tempfile"
    ],
    "Extensions": {
      "SearchPaths": [
        "./extensions",
        "./packages"
      ],
      "FilePatterns": [
        "*.py",
        "*.rpx"
      ],
      "EnableAutoDiscovery": true,
      "EnableValidation": true,
      "RequireSigned": false,
      "TrustedPublishers": [],
      "LoadTimeout": 10000
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "RevitPy": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  }
}